{
  "name": "api-rest-fastify-nodejs",
  "version": "1.0.0",
  "description": "Creating an api using Fastify with NodeJS (Structure, Database, Routes, Tests and Deploy)",
  "main": "src/server.ts",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "start": "tsx src/server.ts",
    "build": "tsc",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "knex": "node --no-warnings --import tsx ./node_modules/knex/bin/cli.js",
    "knex:migrate:latest": "knex -- migrate:latest",
    "knex:migrate:rollback:all": "knex -- migrate:rollback --all"
  },
  "keywords": [
    "tests",
    "fastify",
    "nodejs",
    "api",
    "structure",
    "database",
    "routes",
    "deploy"
  ],
  "author": "Euclides do Ros√°rio <euclidesdry@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@fastify/cookie": "^9.3.1",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.0",
    "knex": "^3.1.0",
    "sqlite3": "^5.1.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "^2.2.2",
    "@types/node": "^20.14.6",
    "@types/supertest": "^6.0.2",
    "eslint": "^8.57.0",
    "supertest": "^7.0.0",
    "tsx": "^4.15.6",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}
